            <!-- END AFFILIATE TABLE -->
        </div>
    </div>
    
    <div id="footer">        
        $[footer]
    </div>
    
    {if $[is_mobile_browser]}
    	<br /><p class="center">Switch to the $[mobile_switch_link].</p>
    {/if}
</body>
</html>
